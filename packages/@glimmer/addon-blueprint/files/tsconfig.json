{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "inlineSourceMap": true,
    "declaration": true,
    "newLine": "LF",
    "types": [
      "qunit"
    ],
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "exclude": [
    "node_modules",
    "tmp",
    "dist"
  ]
}
